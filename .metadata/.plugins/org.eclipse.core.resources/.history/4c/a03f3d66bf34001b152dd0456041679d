package com.cg.attendance.services;

import java.util.Arrays;
import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.cg.attendance.domain.AttendanceDetails;
import com.cg.attendance.repositories.AttendanceDetailsRepository;

@Service
public class AttendanceDetailsService {
	
	@Autowired
	private AttendanceDetailsRepository attendanceDetailsRepository;
	private List<String> attendanceDetails =Arrays.asList("08:30","06:00","02-12-2020","sick","wfh","pending");
  

public List<String> getAllDetails() {
	return attendanceDetails;
}
}